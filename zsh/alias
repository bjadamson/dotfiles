source $ZSH_DOTFILES/replace

# GIT
alias gd='git diff'
alias gdc='gd --cached'
alias gs='git status'
alias gl='git lola'

alias gdo=diff-origin
function diff-origin() {
  git diff origin/"$1"
}

alias gdco=diff-origin-cached
function diff-origin-cached() {
  git diff --cached origin/"$1"
}

alias git-pwn=push-override
function push-override() {
  git push "$1" ':'"$2"
  git push "$1" "$2"
}

alias git-pwno=push-override-origin
function push-override-origin() {
  git push origin ':'"$1"
  git push origin "$1"
}

# ToolChain
alias gcc='gcc-4.9'
alias g++='g++-4.9'

# directory shortcuts
alias cddot='cd ~/Downloads/dotfiles/'

# terminal
alias p='pwd'
alias c='clear'
alias l='ls'
alias hn='hostname'
alias lt='ls -lt'
alias rr='replace $1 $2'
alias s='grep -rl $1 $2'
alias u='cd ..'
alias uu='u && u'
alias uuu='uu && u'
alias uuuu='uuu && u'
alias uuuuu='uuuu && u'
